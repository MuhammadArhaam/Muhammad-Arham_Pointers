#include <iostream>
using namespace std;
int sumArray(int* arr, int size) 
{
    int sum = 0;
    for (int i = 0; i < size; i++) 
    {
        sum += *(arr + i);
    }
    return sum;
}
int main() 
{
    int myArray[5] = {1, 2, 3, 4, 5};
    int total = sumArray(myArray, 5);
    cout << "Sum of array elements= " << total << endl;
    return 0;
}
